	<queryString>
		<![CDATA[SELECT
     CAT_PRODUCTOS.`NOM_PROD` AS descripcion,
	CAT_PRODUCTOS.`CODIGO_ARTICULO` AS codigo,
CAT_PRODUCTOS.`PRECIO_VENTA_PROD` AS precio,    
     CAT_PRODUCTOS.`NOM_PROVEEDOR` AS empresa,
	CAT_PROVEEDORES.`NOM_PROVEEDOR` AS proveedor
     
FROM
   `cat_productos` cat_productos   INNER JOIN `cat_proveedores` cat_proveedores ON cat_proveedores.`ID_PROVEEDOR` = cat_productos.`ID_PROVEEDOR`
WHERE NOM_PROVEEDOR=$P{nomProveedor}]]>
	</queryString>
